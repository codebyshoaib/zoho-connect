{
    "name": "zoho-connect/zoho-flow-connect",
    "description": "WordPress plugin that serializes QuanticaLabs CRBS booking data and sends to Zoho Flow webhook",
    "type": "wordpress-plugin",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=7.4",
        "yahnis-elsts/plugin-update-checker": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ZohoConnectSerializer\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}